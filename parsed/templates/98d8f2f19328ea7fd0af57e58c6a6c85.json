{"components/products/customizations/modal-cart":"<aside class=\"modal-cart\">\n  <div class=\"modal-cart__inner\">\n    <div class=\"modal-cart__top-bar\">\n      <h2 class=\"modal-cart__title\">Added to your Cart</h2>\n      <button class=\"modal-cart__close\">{{> svg/close}}</button>\n    </div>\n    <div class=\"modal-cart__contents\">\n      <div class=\"modal-cart__item-list\">\n        <div class=\"mini-cart-item\" data-product-id=\"{{product.product_id}}\">\n          <div class=\"mini-cart-item-wrap\">\n            {{#if type '==' 'GiftCertificate'}}\n              <a href=\"{{product.edit_url}}\">\n                <svg class=\"giftcard-image\" version=\"1.1\" id=\"Layer_1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\"\n                    viewBox=\"0 0 100 81.9\" style=\"enable-background:new 0 0 100 81.9;\" xml:space=\"preserve\">\n                  <path style=\"fill:currentColor\" d=\"M77.4,0.3L5.8,21.1c-4.3,1.2-6.7,5.7-5.5,10l12.3,42.3c1.2,4.3,5.7,6.7,10,5.5l71.6-20.8\n                    c4.3-1.2,6.7-5.7,5.5-10L87.4,5.8C86.2,1.5,81.7-0.9,77.4,0.3z M84.1,6.8l2.6,8.8L48.1,26.8c-0.7-2.2-1.8-5.4-3.2-7.6\n                    c0,0-7.6,1.7-11,5.6l-2.2-7.6L78.4,3.6C80.8,2.9,83.4,4.3,84.1,6.8z M6.8,24.5l17.4-5.1l2.4,8.1c-5.1-0.3-11.4,3.9-11.4,3.9\n                    c0.4,1.5,1,3,1.6,4.5L6.2,39l-2.6-8.8C2.9,27.7,4.3,25.2,6.8,24.5z M15.9,72.5l-7.6-26l17.1-5c-0.8,2.5-2,5.5-3.6,9.1l5.2,9.4\n                    c0,0,2.5-7.1,4.7-14.7l7.3,25.3l-17.4,5.1C19.2,76.3,16.6,74.9,15.9,72.5z M93.3,54.8L46.5,68.4l-8.3-28.5C45.9,46,55,52.5,55,52.5\n                    L53,42c-3.4-2.3-6.2-4.3-8.4-6l44.3-12.9l7.6,26C97.1,51.5,95.7,54.1,93.3,54.8z\"/>\n                  <ellipse style=\"fill:currentColor\" cx=\"53.6\" cy=\"79.3\" rx=\"41.3\" ry=\"2.6\"/>\n                </svg>\n              </a>\n            {{else}}\n              <a href=\"{{product.url}}\">\n                <div\n                  class=\"mini-cart-item-thumb {{#unless product.main_image}}no-image{{/unless}}\"\n                  style=\"background-image: url({{getImage product.main_image 'thumb' (cdn ../../theme_settings.default_image)}})\">\n                </div>\n                <img\n                  class=\"sr-only\"\n                  src=\"{{getImage product.main_image 'thumb' (cdn ../../theme_settings.default_image)}}\"\n                  alt=\"{{product.main_image.alt}}\"\n                />\n              </a>\n            {{/if}}\n            <div class=\"mini-cart-item-description\">\n              <a href=\"{{product.url}}\" class=\"mini-cart-item-title\">{{product.title}}</a>\n              <div class=\"mini-cart-item-options\">\n                {{#each product.options}}\n                  <span class=\"option-value\" data-name=\"{{display_name}}\">\n                    <span class=\"option-value-label-mc\">\n                        {{#replace \"Select\" display_name}}\n                        Selected\n                      {{/replace}}:\n                    </span>\n                    <span class=\"option-value-wrapper\" data-name=\"{{display_name}}\" data-option-id=\"{{id}}\"></span>\n                  </span>\n                {{/each}}\n              </div>\n            </div>\n            <div class=\"mini-cart-item-price\">\n              <span data-quantity></span> &times;\n              <span data-price></span>\n            </div>\n          </div>\n        </div>\n      </div>\n    </div>\n    <div class=\"modal-cart__summary\">\n        <div class=\"modal-cart__subtotal\">\n            <span class=\"modal-cart__subtotal-label\">Order Subtotal</span>\n            <span class=\"modal-cart__subtotal-value\" data-value=\"{{cart.grand_total.value}}\">{{cart.grand_total.formatted}}</span>\n        </div>\n        <div class=\"modal-cart__count\">\n            <span class=\"modal-cart__count-value\" data-quantity=\"{{cart.quantity}}\">{{cart.quantity}}</span>\n            <span class=\"modal-cart__count-unit\">{{#if cart.quantity \"==\" 1}}item{{else}}items{{/if}}</span>\n        </div>\n        <a class=\"button button-primary button-cart\" href=\"{{urls.cart}}\">\n          <span class=\"button-text\">\n            {{#if theme_settings.cart_icon '==' 'bag'}}\n                {{lang 'mini_cart.view_bag'}}\n            {{else}}\n                {{lang 'mini_cart.view_cart'}}\n            {{/if}}\n          </span>\n        </a>\n        <div class=\"modal-cart__additional-checkout-buttons\"></div>\n        <div class=\"modal-cart__marketing modal-cart__marketing--summary\"></div>\n    </div>\n    <div class=\"modal-cart__marketing modal-cart__marketing--footer\"></div>\n</div>\n  <div class=\"modal-cart__underlay\"></div>\n</aside>\n","svg/close":"<svg class=\"icon-close\" width=\"15\" height=\"15\" viewBox=\"0 0 15 15\" xmlns=\"http://www.w3.org/2000/svg\"><title>close</title><path d=\"M15 1.73L13.52.25 7.89 5.88 2.26.25.778 1.73l5.63 5.63-5.63 5.63 1.482 1.482 5.63-5.63 5.63 5.63 1.48-1.48-5.63-5.63L15 1.73\"/></svg>\n"}
