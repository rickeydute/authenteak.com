{"components/cart/coupon-codes":"{{inject 'couponCodeEmptyInput' (lang 'cart.coupon_codes.error')}}\n\n<div class=\"cart-totals-item coupon-code\" data-coupon-codes>\n  <form class=\"form form-coupon\" method=\"post\" action=\"{{urls.cart}}\" data-coupon-code-form>\n    <p class=\"cart-total-label coupon-code-title\">{{lang 'cart.coupon_codes.title'}}</p>\n    <div class=\"input-group\">\n      <div class=\"form-field input-group-item\">\n        <label class=\"form-label\" for=\"couponcode\">\n          <span class=\"form-label-text sr-only\">{{lang 'cart.coupon_codes.enter_code'}}</span>\n          <input class=\"form-input\" id=\"couponcode\" type=\"text\" name=\"couponcode\" value=\"\" placeholder=\"{{lang 'cart.coupon_codes.enter_code'}}\" data-coupon-code-input>\n        </label>\n      </div>\n      <div class=\"form-actions input-group-item\">\n        <input class=\"button button-tertiary\" type=\"submit\" value=\"{{lang 'cart.coupon_codes.submit'}}\">\n        <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n      </div>\n    </div>\n  </form>\n</div>\n","components/cart/gift-certificates":"<div class=\"cart-totals-item\">\n  {{inject 'giftCertificateInputEmpty' (lang 'cart.gift_certificates.error')}}\n\n  <div data-gift-certificates>\n    <span class=\"cart-total-label\">\n      {{lang 'cart.gift_certificates.gift_certificate'}}\n    </span>\n\n    <span class=\"link\" data-gift-certificate-toggle>\n      {{lang 'cart.gift_certificates.add_code'}}\n    </span>\n\n    <form\n      class=\"form form-gift-certificate\"\n      method=\"post\"\n      action=\"{{urls.cart}}\"\n      data-gift-certificate-form\n    >\n      <label class=\"sr-only form-label\" for=\"certcode\">\n        {{lang 'cart.gift_certificates.enter_code'}}\n      </label>\n      <div class=\"input-group\">\n        <div class=\"form-field input-group-item\">\n          <input\n            class=\"form-input\"\n            id=\"certcode\"\n            type=\"text\"\n            name=\"certcode\"\n            value=\"\"\n            placeholder=\"{{lang 'cart.gift_certificates.enter_code'}}\"\n            data-gift-certificate-input\n          >\n        </div>\n        <div class=\"form-field input-group-item\">\n          <input\n            class=\"button button-tertiary\"\n            type=\"submit\"\n            value=\"{{lang 'cart.gift_certificates.submit'}}\"\n          >\n        </div>\n      </div>\n      <input type=\"hidden\" name=\"action\" value=\"applycoupon\">\n    </form>\n  </div>\n\n  {{#if cart.gift_certificates}}\n    <ul class=\"list-bulleted\">\n      {{#each cart.gift_certificates}}\n        <li>\n          {{lang 'cart.gift_certificates.label' code=code}}\n          <span class=\"cart-total-item\">\n            {{used.formatted}}\n          </span>\n          <div>\n            {{remaining.formatted}} {{lang 'cart.gift_certificates.remaining'}}\n          </div>\n          <a class=\"link\" href=\"{{remove_url}}\">\n            {{lang 'cart.gift_certificates.remove'}}\n          </a>\n        </li>\n      {{/each}}\n    </ul>\n  {{/if}}\n</div>\n","components/cart/totals":"<script\n  id=\"bolt-connect\"\n  type=\"text/javascript\"\n  src=\"https://connect.bolt.com/connect-bigcommerce.js\"\n  data-publishable-key=\"3ximKwUWJJ6Q.SC4loucq5R6G.7c46996f8803dcf5a87ddc1ef5f9a9c866bbcb290f9c4ba450c92ef72081e578\"\n  data-shopping-cart-id=\"BigCommerce\">\n</script>\n\n<div class=\"cart-totals-column\">\n  {{> components/cart/coupon-codes}}\n</div>\n\n<div class=\"cart-totals-column cart-totals-column--last\">\n  <div class=\"cart-sub-total cart-totals-item\">\n    <span class=\"cart-total-label\">{{lang 'cart.sub_total'}}</span>\n    <span class=\"cart-total-item\">{{cart.sub_total.formatted}}</span>\n  </div>\n\n  {{#each cart.taxes}}\n    <div class=\"cart-totals-item\">\n      <span class=\"cart-total-label\">{{name}}:</span>\n      <span class=\"cart-total-item\">{{cost.formatted}}</span>\n    </div>\n  {{/each}}\n\n  {{#if cart.discount.formatted}}\n    <div class=\"cart-totals-item\">\n      <span class=\"cart-total-label\">{{lang 'cart.discount'}}</span>\n      <span class=\"cart-total-item\">{{cart.discount.formatted}}</span>\n    </div>\n  {{/if}}\n\n  {{#if cart.coupons}}\n    <div class=\"cart-totals-item\">\n      <span class=\"cart-total-label\">\n        {{lang 'cart.coupon_codes.coupon_code'}}\n      </span>\n      <ul class=\"coupons-active list-bulleted\">\n        {{#each cart.coupons}}\n          <li>\n            {{lang 'cart.coupon_codes.label' code=code}}\n            <span class=\"cart-total-item\">{{discount.formatted}}</span>\n            <a class=\"link\" href=\"{{remove_url}}\">\n              {{lang 'cart.coupon_codes.remove'}}\n            </a>\n          </li>\n        {{/each}}\n      </ul>\n    </div>\n  {{/if}}\n\n  {{!-- > components/cart/shipping-calculator cart.shipping_handling --}}\n  {{#if settings.gift_certificates_enabled}}\n    {{> components/cart/gift-certificates}}\n  {{/if}}\n\n  {{#if cart.gift_wrapping_cost.value \">\" 0}}\n    <div class=\"cart-totals-item\">\n      <span class=\"cart-total-label\">{{lang 'cart.gift_wrapping.title'}}</span>\n      <span class=\"cart-total-item\">{{cart.gift_wrapping_cost.formatted}}</span>\n    </div>\n  {{/if}}\n\n  <div class=\"cart-grand-total cart-totals-item\">\n    <span class=\"cart-total-label\">{{lang 'cart.grand_total'}}</span>\n    <span class=\"cart-total-item grand-total\">\n      {{cart.grand_total.formatted}}\n    </span>\n  </div>\n  <div class=\"cart-actions\">\n    <!-- <a class=\"button button-secondary button-wide\" href=\"{{urls.home}}\">\n      {{lang 'cart.keep_shopping'}}\n    </a> -->\n   \n    <!-- <a class=\"button button-primary button-wide button-checkout\" href=\"{{urls.checkout.single_address}}\">\n      {{lang 'cart.checkout'}}\n    </a> -->\n\n    <div class=\"bolt-button-wrapper\" style=\"display: contents;\">\n        <!--To change the theme color place the new hex color after --bolt-primary-action-color-->\n      <div class=\"bolt-checkout-button large-width with-cards responsive bolt-multi-step-checkout\" style=\"--bolt-primary-action-color:#6ea438\"></div>\n    </div>\n\n\n    {{#if cart.show_multiple_address_shipping}}\n      <a class=\"checkout-multiple\" href=\"{{urls.checkout.multiple_address}}\">\n        {{lang 'cart.checkout_multiple'}}\n      </a>\n    {{/if}}\n  </div>\n\n  <div class=\"cart-additional-checkout-buttons\">\n    <a href=\"{{urls.checkout.single_address}}\" class=\"cart-afirm-link\">\n      <svg class=\"cart-affirm-btn\"  viewBox=\"0 0 296 60\" enable-background=\"new 0 0 296 60\" xml:space=\"preserve\" version=\"1.1\" xmlns=\"http://www.w3.org/2000/svg\" xmlns:xlink=\"http://www.w3.org/1999/xlink\" x=\"0px\" y=\"0px\">\n        <title>60_296-blue</title>\n        <defs><polygon id=\"b\" points=\"29.708 39.178 95.708 39.178 95.708 12.843 29.708 12.843\"/></defs><g fill=\"none\" fill-rule=\"evenodd\"><g transform=\"translate(-.5 -.5)\"><g transform=\"translate(.52344 .44727)\" fill=\"#0FA0EA\" stroke=\"#0FA0EA\" stroke-width=\"2\"><rect x=\"1\" y=\"1\" width=\"294\" height=\"58\" rx=\"8\"/></g>\n        <text fill=\"#FFFFFF\" font-family=\"Calibre-Semibold, Calibre\" font-size=\"22\" font-weight=\"500\"><tspan x=\"105.59668\" y=\"38.8670156\">Monthly payments</tspan></text>\n        <path d=\"m34.036 36.91c-0.815 0-1.224-0.402-1.224-1.063 0-1.227 1.373-1.646 3.878-1.913 0 1.641-1.109 2.976-2.654 2.976m1.08-9.233c-1.79 0-3.85 0.842-4.968 1.734l1.021 2.149c0.897-0.82 2.347-1.522 3.655-1.522 1.242 0 1.928 0.416 1.928 1.253 0 0.563-0.454 0.848-1.314 0.959-3.211 0.417-5.73 1.304-5.73 3.779 0 1.962 1.397 3.149 3.58 3.149 1.557 0 2.944-0.866 3.603-2.008v1.688h2.904v-7.074c0-2.921-2.031-4.107-4.679-4.107\" fill=\"#fff\"/>\n        <path d=\"m64.266 27.996v10.862h3.107v-5.234c0-2.487 1.506-3.218 2.556-3.218 0.411 0 0.963 0.119 1.328 0.394l0.566-2.872c-0.479-0.206-0.981-0.251-1.392-0.251-1.598 0-2.602 0.707-3.264 2.144v-1.825h-2.901z\" fill=\"#fff\"/>\n        <path d=\"m86.229 27.677c-1.643 0-2.872 0.972-3.511 1.907-0.593-1.21-1.851-1.907-3.357-1.907-1.643 0-2.78 0.912-3.305 1.962v-1.643h-2.995v10.863h3.109v-5.591c0-2.008 1.051-2.971 2.033-2.971 0.888 0 1.704 0.574 1.704 2.058v6.504h3.105v-5.591c0-2.031 1.026-2.971 2.053-2.971 0.822 0 1.688 0.598 1.688 2.035v6.527h3.104v-7.508c0-2.442-1.643-3.674-3.628-3.674\" fill=\"#fff\"/>\n        <path d=\"m56.703 27.996h-2.815v-1.104c0-1.438 0.82-1.849 1.528-1.849 0.782 0 1.391 0.347 1.391 0.347l0.958-2.191s-0.971-0.635-2.737-0.635c-1.986 0-4.245 1.119-4.245 4.633v0.799h-4.712v-1.104c0-1.438 0.82-1.849 1.528-1.849 0.402 0 0.942 0.093 1.391 0.347l0.958-2.191c-0.572-0.335-1.491-0.635-2.737-0.635-1.986 0-4.245 1.119-4.245 4.633v0.799h-1.803v2.396h1.803v8.466h3.105v-8.466h4.712v8.466h3.105v-8.466h2.815v-2.396z\" fill=\"#fff\"/>\n        <mask id=\"a\" fill=\"white\"><use xlink:href=\"#b\"/></mask><polygon points=\"58.556 38.858 61.658 38.858 61.658 27.996 58.556 27.996\" fill=\"#fff\" mask=\"url(#a)\"/><path d=\"m76.652 12.843c-8.387 0-15.861 5.821-17.982 13.306h3.039c1.773-5.573 7.787-10.466 14.943-10.466 8.699 0 16.215 6.622 16.215 16.931 0 2.314-0.299 4.402-0.867 6.245h2.949l0.029-0.102c0.484-1.901 0.73-3.968 0.73-6.143 0-11.497-8.378-19.771-19.056-19.771\" fill=\"#fff\" mask=\"url(#a)\"/></g></g>\n      </svg>\n    </a>\n  </div>\n\n  <div class=\"affirm-cart\"></div>\n\n  {{#if cart.additional_checkout_buttons.length}}\n    <div class=\"cart-additional-checkout-buttons\">\n      {{#each cart.additional_checkout_buttons}}\n        {{{this}}}\n      {{/each}}\n    </div>\n  {{/if}}\n\n  <a class=\"prop-link-new\" href=\"/prop65\" target=\"_blank\">California Proposition 65 Warning Information</a>\n  \n  <div class=\"prop-wrapper prop-link\">\n    <h4>California Proposition 65 Warning Information</h4>\n    <p class=\"prop-text\">California's Proposition 65 entitles California consumers to special warnings for certain products that contain chemicals above certain threshold levels.</p>\n    <p>⚠WARNING: Furniture products can expose you to chemicals including lead which is known to the State of California to cause cancer or birth defects or other reproductive harm.</p>\n    <p>⚠WARNING: Outdoor heating and fire products can expose you to chemicals including carbon dioxide which is known to the State of California to cause cancer or birth defects or other reproductive harm.</p>\n    <p><a href=\"/prop65\">Learn more</a> about California Proposition 65.</p>\n        \n    <h5>For Weber Grills</h5>\n    <p>This product can expose you to chemicals including nickel, which is known to the State of California to cause cancer, birth defects, or other reproductive harm.  For more information go to <a href=\"http://www.P65Warnings.ca.gov\" target=\"_blank\" title=\"P65 Warnings\">www.P65Warnings.ca.gov</a>.</p>\n  </div>\n</div>\n\n<script>\n/**\n * NOTE: this is a hack.\n * This is only a hack.\n * This needs to be done at the paypal level becasue...it's a hack.\n * This simply replaes the amazon button with a new one\n*/\n\n(function(document, window){\n  let amazonBtn;\n\n  const change = {\n    chainAsync: fns => { let curr = 0;  const next = () => fns[curr++](next); next(); },\n    \n    init: () => {\n      change.chainAsync([\n        next => { setTimeout(swapButtonAmazon, 2000);  setTimeout(next, 2001); },\n        next => { setTimeout(showButtonAmazon, 2000); },\n      ]);\n    }\n  }; \n\n\n  // swap out the amazon pay button ~ kamakizi style\n  function swapButtonAmazon(){\n    if (!window.OffAmazonPayments){ throw \"SCRIPT_NOT_LOADED\";}\n\n    amazonBtn = document.getElementById(\"CheckoutButtonAmazon0\").querySelector(\".amazonpay-button-inner-image\");\n    amazonBtn.src = \"//authenteak.com/content/amazon-pay-btn.png\";\n    amazonBtn.srcset = \"//authenteak.com/content/amazon-pay-btn.png\";\n    amazonBtn.style.opacity = 1;\n  }\n\n\n  function showButtonAmazon(){\n    document.getElementById(\"OffAmazonPaymentsWidgets0\").classList.add(\"amazonpay-button-inner-image--show\");\n\n    amazonBtn.addEventListener(\"click\", function(){\n      var subTotal = \"{{cart.sub_total.formatted}}\".replace(/\\$|,/g, ''),\n          qty = \"{{cart.quantity}}\";\n\n      subTotal = parseInt(subTotal);\n      qty = parseInt(qty);\n\n      TEAK.Modules.fbPixel.checkoutStart(subTotal, qty);\n    });\n\n  }\n\n  // Get the cart detail from localstorage\n  function getStoredCart(){\n    let storedCart = localStorage.getItem('cartData');\n    \n    return JSON.parse(storedCart);\n  }\n\n  function getCartQnty(cart){\n    let count = 0;\n\n    cart[0].lineItems.physicalItems.forEach(element => {\n      count += element.quantity;\n    });\n\n    return count;\n  }\n\n  // update the cart header\n  function updateHeaderMiniCart(){\n    let $dataCart = $(\"[data-cart-quantity]\"),\n        cart = getStoredCart(),\n        cartQty = getCartQnty(cart);\n\n    $dataCart.data(\"cartQuantity\", cartQty);\n    $dataCart.html(cartQty);\n  }\n\n  // wait until the amazon script has loaded.\n  try {\n    change.init();\n\n  } catch (error) {\n    if (error === 'SCRIPT_NOT_LOADED' && document.getElementById(\"OffAmazonPaymentsWidgets0\")) {\n      document.addEventListener('DOMContentLoaded', change.init);\n    \n    }else{\n      throw \"SCRIPT_NOT_LOADED\";\n    }\n  }\n\n  document.addEventListener('DOMContentLoaded', function() {\n    $(window).on(\"cartDataStored\", updateHeaderMiniCart);\n  });\n\n}(document, window));\n</script>\n\n<script>\n    var originalCheckoutButton = document.querySelector(\".cart-actions .button-primary\");\n    var boltButtons = document.getElementsByClassName(\"bolt-button-wrapper\");\n    \n    if (true) {\n        for (let i = 0; i < boltButtons.length; i++) {\n           boltButtons[i].style.display = 'contents';\n    \n          // Facebook Pixel track for Bolt - initate checkout\n          // cannot track 'AddPaymentInfo' because bolt is in a iframe\n          boltButtons[i].addEventListener(\"click\", function(){\n            let subTotal = \"{{cart.sub_total.formatted}}\".replace(/\\$|,/g, ''),\n                qty = \"{{cart.quantity}}\";\n    \n            subTotal = parseInt(subTotal);\n            qty = parseInt(qty);\n    \n            TEAK.Modules.fbPixel.checkoutStart(subTotal, qty);\n          });\n        }\n        \n        if(originalCheckoutButton){\n          originalCheckoutButton.style.display = 'none';\n        }\n    }\n    \n    var interval = setInterval(function(){\n    if (window.BoltCheckout) {\n        window.BoltCheckout.setClientCustomCallbacks({\n            success : function(trx){\n                fbq('track', 'Purchase', {\n                    value: trx.amount.amount / 100.0,\n                    currency: 'USD',\n                });\n            }\n        });\n        clearInterval(interval);\n    }\n    }, 200);\n    \n    var config = { childList: true, subtree: true };\n      // Change totalClassName\n    var totalClassName = \"grand-total\";\n    var totalPrice = \"\";\n    var callback = function(mutationsList) {\n        var elms = document.getElementsByClassName(totalClassName);\n        if (elms.length == 0) {\n            return;\n        }\n        var newPrice = elms[0].innerHTML;\n        if (newPrice !== totalPrice && window.BoltCheckout) {\n            window.BoltCheckout.reloadBigCommerceCart();\n        }\n        totalPrice = newPrice;\n    };\n    new MutationObserver(callback).observe(document.body, config);\n    \n  </script>\n    \n"}
